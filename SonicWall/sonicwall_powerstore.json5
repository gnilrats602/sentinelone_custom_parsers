{
  "patterns": {
    "syslog_priority": "[0-9]{1,3}",
    "attribPattern": "[a-zA-Z0\\-_]+", // Only extract attributes with alphanumeric and hyphen / underscore characters
    "valuePattern1": "[^\\s|\"|\']+", // Extract all values that are not spaces, double quotations, or single quotations
    "valuePattern2": "[^\"]+", // Extract all values that are not double quotations
    "valuePattern3": "[^\']+", // Extract all values that are not single quotations
    "tsPattern": "[A-Z][a-z]{2}\\s[0-9]{1,2}\\s[0-9]{2}:[0-9]{2}:[0-9]{2}", // Extract timestamp in the format Aug 02 20:09:13--7:00
    "ipPattern": "((\\d+\\.){3}\\d+|[a-zA-Z0-9:]+)", // Extract IPv4 or IPv6 addresses
    "hostShort": "[A-Za-z0-9_.\\-]+",
    "pidNum": "[0-9]+",
    "isoTs": "[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}",
  },
  "formats": [
    {
      format: "<$syslog_priority$>$ts=tsPattern$\\s+$hostname=hostShort$\\s+\\[$pid=pidNum$\\]: .* Config", // We apply patterns at attribute level. This format rule applies to second example
      rewrites: [
        {
          input:   "ts", // 1.) Timestamp + timezone was extracted to buffer variable (ts) for further modification. Use ts as input.
          output:  "timestamp", // 4.) Assign extracted + replaced value to reserved timestamp attribute
          match:   "(\\w+ \\d+ \\d{2}:\\d{2}:\\d{2})\\-\\-\\d+:\\d+", // 2.) Extract timestamp from ts
          replace: "$1" // 3.) Replace field with regex group 1
        },
      ]
    },
    {
      format: ".*$_=identifier$=$_=quoteOrSpace$",
      repeat: true
    }
  ],

	"mappings": {
  	"version": 0,
  	"mappings": [
    {
      predicate: "hostname = 'APX00240006095-B'",
      repeat: false,
      constants: [
        { value: "PowerStore Audit Event", field: "event.type" }
      ]
    }
  ]
}

}